{
  "compilerOptions": {
    "strict": false,
    "allowJs": false,
    "outDir": ".",
    "baseUrl": "./src",
    "forceConsistentCasingInFileNames": true,
    "target": "ESNext",
    "lib": [
      "ESNext"
    ],
    "moduleResolution": "classic",
    "paths": {
      "*": [
        "../node_modules/*/src/index",
        "../node_modules/*/lib/index",
        "../node_modules/*/index",
        "../node_modules/*/dist/index",
        "../node_modules/*/dist/cjs/index",
        "../node_modules/*"
      ]
    },
    "plugins": [
      {
        "transform": "war3-transformer",
        "mapDir": "E:/codename-askellon/maps/askellon-sector.w3x",
        "entryFile": "E:/codename-askellon/src/main.ts",
        "outputDir": "E:/codename-askellon/dist/askellon-sector.w3x"
      }
    ],
    "types": [
      "typescript-to-lua/language-extensions",
      "lua-types/core/coroutine",
      "lua-types/core/global",
      "lua-types/core/math",
      "lua-types/core/metatable",
      "lua-types/core/modules",
      "lua-types/core/string",
      "lua-types/core/table",
      "lua-types/core/os",
      "lua-types/special/5.3",
      "war3-types-strict/1.32.10",
      "war3-transformer/types",
      "war3-objectdata/dist/cjs/objectdata"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [],
  "tstl": {
    "luaTarget": "5.3",
    "noHeader": true,
    "luaLibImport": "require",
    "noImplicitSelf": true,
    "luaBundle": "dist/tstl_output.lua",
    "luaBundleEntry": "./src/main.ts",
    "sourceMapTraceback": false
  }
}